<!DOCTYPE html>
<html>
<head>
  <title>TestRorProject</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <script src="http://maps.google.com/maps?file=api&v=2&key=AIzaSyAbfckxG2R4hsRBmagoD12PQBmOrIlqV7k" type="text/javascript"></script>
</head>
<body>
	<%= render 'layouts/header' %>
	<div class="container">
	      <% flash.each do |key, value| %>
	        <div class="alert alert-<%= key %>"><%= value %></div>
	      <% end %>
	      <%= yield %>
	      <%= render 'layouts/footer' %>
	 </div>
</body>
</html>
