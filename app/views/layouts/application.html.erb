<!DOCTYPE html>
<html>
<head>
  <title>TestRorProject</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <script type="text/javascript"
      src="http://maps.googleapis.com/maps/api/js?key=AIzaSyBzqDHsG4JId3YRI5Rssp8_zMzLnj2bpyg&sensor=false">
   </script>
</head>
<body>
	<%= render 'layouts/header' %>
	<div class="container">
	      <% flash.each do |key, value| %>
	        <div class="alert alert-<%= key %>"><%= value %></div>
	      <% end %>
	      <%= yield %>
	      <%= render 'layouts/footer' %>
	 </div>
</body>
</html>
